;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	dev.irex.aretex.ca. root.dev.irex.aretex.ca. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	localhost.
1.0.0	IN	PTR	localhost.

;Name server information

@	IN	NS	dns-dev.dev.irex.aretex.ca.

;Reverse lookup for Name server

10	IN	PTR	dns-dev.dev.irex.aretex.ca.

;PTR Record ip address to hostname

216	IN	PTR	bbb.dev.irex.aretex.ca.
241	IN	PTR	drive.dev.irex.aretex.ca.
224     IN      PTR     gate.dev.irex.aretex.ca.
;224     IN      A     guacamole.dev.irex.aretex.ca;192.168.33.224

